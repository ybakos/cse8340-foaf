@base             <http://lyle.smu.edu/cse8340#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:     <http://xmlns.com/foaf/spec/> .
@prefix frbr:     <http://purl.org/vocab/frbr/core#> .
@prefix dc:       <http://purl.org/dc/elements/1.1/> .
@prefix film:     <http://data.linkedmdb.org/page/movie/film/> .
@prefix actor:    <http://data.linkedmdb.org/resource/actor/> .
@prefix rel:      <http://purl.org/vocab/relationship/> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix :         <#> .


<>                dc:date               "2015-06-13"^^xsd:date ;
                  rdfs:label            "FOAF Profile" ;
                  rdfs:comment          "A FOAF Profile with additional requirements for CSE 8340 Assignment 3." ;
                  rdfs:seeAlso          :assignment3 .

:Musician         a                     rdfs:Class ;
                  rdfs:subClassOf       foaf:Person .

:Parent           a                     rdfs:Class ;
                  rdfs:subClassOf       foaf:Person ;
                  rdfs:comment          "A person who has children." .

:Child            owl:inverseOf         :Parent .

:numberOfChildren a                     rdfs:Property ;
                  rdfs:domain           :Parent ;
                  rdfs:range            xsd:integer .

:numberOfBoys     a                     rdfs:Property ;
                  rdfs:subPropertyOf    :numberOfChildren .

:z                a                     foaf:Person .

:drc              a                     foaf:Person .

:jimi             a                     :Musician ;
                  foaf:name             "Jimi Hendrix" ;
                  :alive                false .

:johnnyHendrix    a                     foaf:Person ;
                  foaf:name             "Johnny Allen Hendrix" ;
                  owl:sameAs            :jimi .

[]                a                     frbr:Work ;
                  dc:creator            :jimi ;
                  dc:title              "Castles Made of Sand" .


:billAndTed       dc:title              "Bill & Ted's Excellent Adventure" ;
                  owl:sameAs            film:390 .

actor:29536       rdf:type              foaf:Person ;
                  foaf:familyName       "Reeves" ;
                  foaf:givenName        "Keanu" .

:yongb            a                     foaf:Person ;
                  foaf:familyName       "Bakos" ;
                  foaf:givenName        "Yong" ;
                  :likes                :billAndTed ;
                  rel:knowsByReputation :jimi, actor:29536 ;
                  rel:knowsOf           :jimi, actor:29536 ;
                  rel:wouldLikeToKnow   :jimi ;
                  rel:influencedBy      :drc ;
                  rel:colleagueOf       :drc ;
                  foaf:knows            :z ;
                  foaf:age              39 .

:rich             a                     rdf:Statement ;
                  rdf:subject           foaf:Person ;
                  rdf:predicate         :income ;
                  rdf:object            0.2 .

foaf:knows        a                     owl:SymmetricProperty .
rel:colleagueOf   a                     owl:SymmetricProperty .
rel:influencedBy  a                     owl:TransitiveProperty .
rel:knowsByReputation owl:equivalentProperty rel:knowsOf .
foaf:age          a                     owl:FunctionalProperty .
